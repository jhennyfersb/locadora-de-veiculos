DROP TABLE PASSWORD_RESET_TOKEN;
create table PASSWORD_RESET_TOKEN
(
    ID_PASSWORD_RESET_TOKEN NUMBER        not null
        primary key,
    ID_FUNCIONARIO          NUMBER        not null
        constraint FK_FUNCIONARIO_RESET_TOKEN
            references FUNCIONARIO,
    TOKEN                   VARCHAR2(200) not null
);

CREATE SEQUENCE SEQ_PRT
    START WITH 1
    INCREMENT BY 1
    NOCACHE NOCYCLE;
